{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "./docs",
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/test/**/*",
    "**/node_modules/**/*"
  ],
  "plugin": [],
  "readme": "./README.md",
  "includeVersion": true,
  "sort": ["source-order"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "categorizeByGroup": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "cleanOutputDir": true
}
